        <cd:Unit>mW.sr-1.m-2.mm-1</cd:Unit>
        <cd:BitSize>16</cd:BitSize>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
</cd:Record>
